####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/bin/ || $(MKDIR) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman/usr/bin/$(QMAKE_TARGET)"
	-$(STRIP) "$(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman/usr/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman/usr/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/bin/ 


install_qm: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/qpacman/ || $(MKDIR) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/qpacman/ 
	-$(INSTALL_FILE) /home/alex/Downloads/qpacman_pkg/src/QPacmanTray/translations/*.qm $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/qpacman/


uninstall_qm:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/qpacman/*.qm
	-$(DEL_DIR) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/qpacman/ 


install_desktop: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/applications/ || $(MKDIR) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/applications/ 
	-$(INSTALL_PROGRAM) /home/alex/Downloads/qpacman_pkg/src/QPacmanTray/QPacmanTray.desktop $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/applications/
	-strip $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/applications/QPacmanTray.desktop


uninstall_desktop:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/applications/QPacmanTray.desktop
	-$(DEL_DIR) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/applications/ 


install_icon: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/pixmaps/ || $(MKDIR) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/pixmaps/ 
	-$(INSTALL_PROGRAM) /home/alex/Downloads/qpacman_pkg/src/QPacmanTray/pics/PacmanTray-arch_logo.png $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/pixmaps/
	-strip $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/pixmaps/PacmanTray-arch_logo.png


uninstall_icon:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/pixmaps/PacmanTray-arch_logo.png
	-$(DEL_DIR) $(INSTALL_ROOT)/home/alex/Downloads/qpacman_pkg/pkg/qpacman//usr/share/pixmaps/ 


install:  install_target install_qm install_desktop install_icon  FORCE

uninstall: uninstall_target uninstall_qm uninstall_desktop uninstall_icon   FORCE

FORCE:

